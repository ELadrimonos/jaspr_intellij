<templateSet group="Jaspr">
    <template name="stlessc" value="class $NAME$ extends StatelessComponent {&#10;  const $NAME$({super.key});&#10;&#10;  @override&#10;  Component build(BuildContext context) {&#10;    return const div([$END$]);&#10;  }&#10;}&#10;" description="New Stateless component" toReformat="false" toShortenFQNames="true">
