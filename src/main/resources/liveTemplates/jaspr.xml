        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <variable name="SHOULD_NOTIFY" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="JASPR_TOPLEVEL" value="true" />
        </context>
    </template>
    <template name="stanimc" value="class $NAME$ extends StatefulComponent {&#10;  const $NAME$({super.key});&#10;&#10;  @override&#10;  State&lt;$NAME$&gt; createState() =&gt; _$NAME$State();&#10;}&#10;&#10;class _$NAME$State extends State&lt;$NAME$&gt; with SingleTickerProviderStateMixin {&#10;  late AnimationController _controller;&#10;&#10;  @override&#10;  void initState() {&#10;    super.initState();&#10;    _controller = AnimationController(vsync: this);&#10;  }&#10;&#10;  @override&#10;  void dispose() {&#10;    _controller.dispose();&#10;    super.dispose();&#10;  }&#10;&#10;  @override&#10;  Component build(BuildContext context) {&#10;    return const Placeholder($END$);&#10;  }&#10;}&#10;" description="New Stateful component with AnimationController" toReformat="false" toShortenFQNames="true">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <context>
            <option name="JASPR_TOPLEVEL" value="true" />
        </context>
    </template>
    <template name="jhtml" value="([$CONTENT$])," description=" nsert a html component" toReformat="false" toShortenFQNames="true">
