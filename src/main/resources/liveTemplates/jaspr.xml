    </template>
    <template name="stfulc" value="class $NAME$ extends StatefulComponent {&#10;  const $NAME$({super.key});&#10;&#10;  @override&#10;  State&lt;$NAME$&gt; createState() =&gt; $SNAME$();&#10;}&#10;&#10;class $SNAME$ extends State&lt;$NAME$&gt; {&#10;  @override&#10;  Component build(BuildContext context) {&#10;    return const div([$END$]);&#10;  }&#10;}&#10;" description="New Stateful component" toReformat="false" toShortenFQNames="true">
